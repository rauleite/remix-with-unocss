{
  "private": true,
  "name": "remix",
  "version": "0.0.1",
  "sideEffects": false,
  "scripts": {
    "build": "npx concurrently --group 'npm:build:*'",
    "build:css": "npm run generate:css",
    "build:remix": "remix build",
    "deploy": "wrangler publish",
    "dev": "concurrently 'npm:dev:*'",
    "dev:css": "npm run generate:css --watch",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "generate:css": "npx unocss './app/**/*.{js,ts,jsx,tsx}' -o ./app/styles/uno.css",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "test": "playwright test",
    "typecheck": "tsc",
    "log": "npm run '/^log:.*/'",
    "log:deploy": "wrangler tail"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.14.3",
    "@remix-run/cloudflare-workers": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230321.0",
    "@playwright/test": "^1.32.1",
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@types/node": "^18.15.9",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@unocss/cli": "^0.50.6",
    "@unocss/reset": "^0.50.6",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "miniflare": "^2.13.0",
    "npm-run-all": "^4.1.5",
    "turbo": "^1.8.5",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
    "vite": "^4.2.1",
    "wrangler": "^2.13.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  }
}
